{
  "name": "@anyalt/widget",
  "version": "1.0.2-canary.52",
  "description": "Anyalt Widget",
  "private": false,
  "files": [
    "dist",
    "package.json"
  ],
  "sideEffects": false,
  "main": "dist/src/index.js",
  "module": "dist/src/index.mjs",
  "types": "dist/src/index.d.ts",
  "peerDependencies": {
    "@tanstack/react-query": ">=5.0.0",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx --no-cache --fix",
    "lint:prettier": "prettier --write 'src/**/*.{ts,tsx}' --fix",
    "lint:tsc": "tsc --noEmit",
    "lint:all": "npm-run-all lint lint:prettier lint:tsc",
    "test": "jest",
    "prepare": "husky install",
    "type-check": "tsc --noEmit",
    "build": "npm run clean && npm run build:types && npm run build:js",
    "build:js": "npm run build:esm && npm run build:cjs",
    "build:esm": "swc src -d dist --config-file .swcrc.esm --out-file-extension mjs",
    "build:cjs": "swc src -d dist --config-file .swcrc",
    "build:types": "tsc --emitDeclarationOnly",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/web3": "^1.20.0",
